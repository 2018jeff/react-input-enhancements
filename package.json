{
  "name": "react-input-enhancements",
  "version": "0.4.12",
  "description": "Set of enhancements for input control (bootstrap-ready)",
  "scripts": {
    "build:lib": "NODE_ENV=production babel src --out-dir lib",
    "build:demo": "NODE_ENV=production webpack -p --optimize-dedupe",
    "stats": "webpack --profile --json > stats.json",
    "start": "webpack-dev-server",
    "lint": "eslint src",
    "version": "npm run build:demo && git add -A .",
    "postversion": "git push",
    "prepublish": "npm run build:lib"
  },
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/alexkuz/react-input-enhancements"
  },
  "keywords": [
    "react",
    "reactjs",
    "input",
    "autocomplete",
    "autosize",
    "typeahead",
    "mask",
    "dropdown",
    "combobox"
  ],
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "export-files-webpack-plugin": "0.0.1",
    "expose-loader": "^0.7.1",
    "html-webpack-plugin": "^2.21.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
