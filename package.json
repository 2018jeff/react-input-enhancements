    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "react": ">=15.1.0",
    "react-addons-perf": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": ">=15.1.0",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.13.1",
